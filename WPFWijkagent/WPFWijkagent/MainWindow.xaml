<Window x:Class="WPFWijkagent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFWijkagent"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="wpf_lb_delicten" Margin="600,23,0,0" SelectionChanged="wpf_lb_delicten_SelectionChanged"/>
        <Rectangle HorizontalAlignment="Center" Height="23" Stroke="Black" VerticalAlignment="Top" Width="800"/>
        <ComboBox x:Name="wpf_cb_categoriesFilter" HorizontalAlignment="Left" Height="23" Margin="295,0,0,0" VerticalAlignment="Top" Width="140" SelectionChanged="wpf_cb_categories_SelectionChanged">
        </ComboBox>
        <TextBlock x:Name="TestTextBlock" HorizontalAlignment="Left" Height="135" Margin="10,28,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Width="180"/>
    </Grid>
</Window>
